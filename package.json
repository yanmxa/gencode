    "ink": "^6.6.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "openai": "^6.16.0",
    "ora": "^9.0.0",
    "react": "^19.2.3",
    "undici": "^7.18.2",
    "zod": "^4.3.5"
