{
  "name": "gencode-ai",
  "version": "0.3.0",
  "description": "An open-source AI assistant for your terminal",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "gencode": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli/index.js",
    "start:dev": "npx tsx src/cli/index.tsx",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "example": "npx tsx examples/basic.ts",
    "migrate": "npx tsx scripts/migrate.ts"
  },
  "keywords": [
    "agent",
    "llm",
    "openai",
    "anthropic",
    "gemini",
    "ai"
  ],
  "author": "Meng Yan",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@google/generative-ai": "^0.24.1",
    "@types/turndown": "^5.0.6",
    "chalk": "^5.6.2",
    "dotenv": "^17.2.3",
    "fast-glob": "^3.3.3",
    "glob": "^13.0.0",
    "google-auth-library": "^10.5.0",
    "gray-matter": "^4.0.3",
    "ink": "^6.6.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "marked": "^17.0.1",
    "minimatch": "^10.1.1",
    "openai": "^6.16.0",
    "ora": "^9.0.0",
    "react": "^19.2.3",
    "turndown": "^7.2.2",
    "undici": "^7.18.2",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/minimatch": "^5.1.2",
    "@types/node": "^25.0.8",
    "@types/react": "^19.2.8",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
